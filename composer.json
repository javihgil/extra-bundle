{
    "name": "javihgil/extra-bundle",
    "description": "Some Symfony extra tools",
    "type": "bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Jhg\\ExtraBundle\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "require": {
        "symfony/config": "~2.5|~3.0",
        "symfony/form": "~2.5|~3.0",
        "symfony/http-foundation": "~2.5|~3.0",
        "symfony/http-kernel": "~2.5|~3.0",
        "symfony/dependency-injection": "~2.5|~3.0"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}